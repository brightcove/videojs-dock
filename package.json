{
  "name": "videojs-dock",
  "version": "2.2.0",
  "description": "Dock plugin for videojs",
  "main": "dist/videojs-dock.cjs.js",
  "keywords": [
    "dock",
    "videojs",
    "videojs-plugin"
  ],
  "author": "Brightcove, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/brightcove/videojs-dock"
  },
  "module": "dist/videojs-dock.es.js",
  "generator-videojs-plugin": {
    "version": "7.3.2"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm-run-all -p build:*",
    "build:css": "node-sass src/plugin.scss dist/videojs-dock.css --output-style=compressed --linefeed=lf",
    "build:js": "rollup -c scripts/rollup.config.js",
    "clean": "shx rm -rf ./dist ./test/dist",
    "postclean": "shx mkdir -p ./dist ./test/dist",
    "docs": "doctoc README.md",
    "lint": "vjsstandard",
    "server": "karma start scripts/karma.conf.js --singleRun=false --auto-watch",
    "start": "npm-run-all -p server watch",
    "pretest": "npm-run-all lint build",
    "test": "karma start scripts/karma.conf.js",
    "update-changelog": "conventional-changelog -p videojs -i CHANGELOG.md -s",
    "preversion": "npm test",
    "version": "is-prerelease || npm run update-changelog && git add CHANGELOG.md",
    "watch": "npm-run-all -p watch:*",
    "watch:css": "npm run build:css -- -w",
    "watch:js": "npm run build:js -- -w",
    "posttest": "shx cat test/dist/coverage/text.txt",
    "prepublishOnly": "npm run build && vjsverify"
  },
  "vjsstandard": {
    "ignore": [
      "dist",
      "docs",
      "test/dist"
    ]
  },
  "files": [
    "CONTRIBUTING.md",
    "dist/",
    "docs/",
    "index.html",
    "scripts/",
    "src/",
    "test/"
  ],
  "dependencies": {
    "global": "^4.4.0",
    "video.js": "^7.17.0"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-videojs": "^3.0.2",
    "doctoc": "^2.1.0",
    "husky": "^7.0.4",
    "karma": "^3.0.0",
    "lint-staged": "^8.0.5",
    "node-sass": "^7.0.0",
    "not-prerelease": "^1.0.1",
    "npm-merge-driver-install": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pkg-ok": "^2.3.1",
    "postcss-cli": "^6.0.0",
    "rollup": "^2.61.1",
    "shx": "^0.3.3",
    "sinon": "^12.0.1",
    "videojs-generate-karma-config": "~5.0.0",
    "videojs-generate-postcss-config": "^2.1.1",
    "videojs-generate-rollup-config": "^7.0.0",
    "videojs-generator-verify": "^1.2.0",
    "videojs-standard": "~8.0.1"
  },
  "lint-staged": {
    "*.js": [
      "vjsstandard --fix",
      "git add"
    ],
    "README.md": [
      "npm run docs:toc",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
